{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "runbot-desktop",
  "version": "0.1.0",
  "identifier": "com.niuhuan.runbot-desktop",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "runbot-desktop",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
        "csp": "default-src 'self'; connect-src 'self' ipc://localhost; img-src 'self' asset: https://asset.localhost data: http://q.qlogo.cn http://p.qlogo.cn https:;"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": []
  }
}
